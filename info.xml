<?xml version="1.0"?>
<extension key="au.com.agileware.xeroitems" type="module">
  <file>xeroitems</file>
  <name>XeroItems</name>
  <description>Allows you to specify ItemCodes instead of AccountCodes when Syncing invoices to Xero.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Agileware</author>
    <email>support@agileware.com.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/agileware/au.com.agileware.xeroitems</url>
    <url desc="Documentation">https://github.com/agileware/au.com.agileware.xeroitems</url>
    <url desc="Support">https://www.agileware.com.au</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2021-10-25</releaseDate>
  <version>1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Depends on nz.co.fuzion.civixero</comments>
  <civix>
    <namespace>CRM/Xeroitems</namespace>
    <format>25.01.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
